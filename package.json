{
  "name": "@buggyorg/graphtools",
  "version": "0.3.2",
  "description": "Tools for processing buggy graphs.",
  "main": "lib/api.js",
  "scripts": {
    "test": "node node_modules/standard/bin/cmd.js src/**/*.js && node_modules/mocha/bin/mocha --compilers js:babel-register",
    "build": "babel src -d lib",
    "doc": "jsdoc -c .jsdoc.json",
    "publish-docs": "jsdoc -c .jsdoc.json && git subtree split --prefix jsdoc -b gh-pages && git push -f origin gh-pages:gh-pages && git branch -D gh-pages",
    "prepublish": "babel src -d lib"
  },
  "keywords": [
    "Buggy"
  ],
  "author": "Maximilian Klein",
  "license": "MIT",
  "dependencies": {
    "deprecate": "^0.1.0",
    "graphlib": "^2.1.0",
    "json-query": "^2.1.0",
    "lodash": "^4.6.1",
    "object-hash": "^1.1.2"
  },
  "devDependencies": {
    "@buggyorg/dupjoin": "^0.1.17",
    "@buggyorg/npg-port-remodeler": "^0.1.12",
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.4.1",
    "jsdoc": "^3.4.0",
    "mocha": "^2.3.3",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "standard": "^6.0.8"
  }
}
